    "react-native-geolocation-service": "^3.1.0",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-onesignal": "^3.4.2",
    "react-native-permissions": "^1.2.1",
    "react-native-responsive-screen": "^1.3.0",
    "react-native-switch-toggle": "^1.1.0",
    "react-native-vector-icons": "^6.5.0",
    "react-navigation": "^3.11.0",
    "react-redux": "^7.0.3",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/runtime": "^7.4.5",
    "babel-jest": "^24.8.0",
    "jest": "^24.8.0",
    "metro-react-native-babel-preset": "^0.54.1",
    "mocha": "^6.1.4",
    "react-test-renderer": "16.8.3",
    "redux-devtools": "^3.5.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "detox": {
    "test-runner": "mocha",
    "specs": "e2e",
    "runner-config": "e2e/mocha.opts",
    "configurations": {
      "ios": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/santteDr.app",
        "build": "xcodebuild -project ios/santteDr.xcodeproj -scheme santteDr -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone X"
      },
      "ios.sim": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/santteDr.app",
        "build": "xcodebuild -workspace ios/santteDr.xcworkspace  -scheme santteDr -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone X"
      },
      "android": {
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "cd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && cd ..",
        "type": "android.emulator",
        "name": "Nexus_2XL_API_28"
      },
      "android.release": {
        "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
        "build": "cd android && ./gradlew assembleRelease assembleAndroidTest -DtestBuildType=release && cd ..",
        "type": "android.emulator",
        "name": "Nexus_2XL_API_28"
      },
      "android.device": {
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "cd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && cd ..",
        "type": "android.attached",
        "name": "LMX410I7SCTSJVZ94S",
        "session": {
          "server": "ws://localhost:8089",
          "sessionId": "YourProjectSessionId"
        }
      }
    }
  }
}
